  margin-bottom: 5px;
  font-size: 16px;
}

.jGrowl-notification .jGrowl-close {
  font-weight: 300;
  background: 0 0;
  border: 0;
  font-size: 17px;
  cursor: pointer;
  line-height: 1;
  padding: 0;
  float: right;
  color: inherit;
  outline: 0;
  margin-left: 10px;
  opacity: .75;
  filter: alpha(opacity=75);
}

