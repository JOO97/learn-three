  from {
    opacity: 0;
    filter: alpha(opacity=0);
  }
  to {
    opacity: 0.01;
    filter: alpha(opacity=1);
  }
}
@-webkit-keyframes page-banner-img {
  from {
    opacity: 0;
    filter: alpha(opacity=0);
    -webkit-transform: scale(1, 1);
    -moz-transform: scale(1, 1);
    transform: scale(1, 1);
  }
  10% {
    opacity: 0.6;
    filter: alpha(opacity=60);
    -webkit-transform: scale(1, 1);
    -moz-transform: scale(1, 1);
    transform: scale(1, 1);
  }
  to {
    opacity: 1;
    filter: alpha(opacity=100);
    -webkit-transform: scale(1.1, 1.1);
    -moz-transform: scale(1.1, 1.1);
    transform: scale(1.1, 1.1);
  }
}
@-moz-keyframes page-banner-img {
  from {
    opacity: 0;
    filter: alpha(opacity=0);
    -webkit-transform: scale(1, 1);
    -moz-transform: scale(1, 1);
    transform: scale(1, 1);
  }
  10% {
    opacity: 0.6;
    filter: alpha(opacity=60);
    -webkit-transform: scale(1, 1);
    -moz-transform: scale(1, 1);
    transform: scale(1, 1);
  }
  to {
    opacity: 1;
    filter: alpha(opacity=100);
    -webkit-transform: scale(1.1, 1.1);
    -moz-transform: scale(1.1, 1.1);
    transform: scale(1.1, 1.1);
  }
}
@keyframes page-banner-img {
  from {
    opacity: 0;
    filter: alpha(opacity=0);
    -webkit-transform: scale(1, 1);
    -moz-transform: scale(1, 1);
    transform: scale(1, 1);
  }
