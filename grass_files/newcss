.animate-swing {
  -webkit-animation: animate-swing 0.3s infinite;
  -moz-animation: animate-swing 0.3s infinite;
  animation: animate-swing 0.3s infinite;
}

@-webkit-keyframes animate-swing {
  from {
    -webkit-transform: rotate3d(0, 0, 1, 1deg);
    -moz-transform: rotate3d(0, 0, 1, 1deg);
    transform: rotate3d(0, 0, 1, 1deg);
  }
  50% {
    -webkit-transform: rotate3d(0, 0, 1, -1deg);
    -moz-transform: rotate3d(0, 0, 1, -1deg);
    transform: rotate3d(0, 0, 1, -1deg);
  }
  to {
    -webkit-transform: rotate3d(0, 0, 1, 1deg);
    -moz-transform: rotate3d(0, 0, 1, 1deg);
    transform: rotate3d(0, 0, 1, 1deg);
  }
}
@-moz-keyframes animate-swing {
  from {
    -webkit-transform: rotate3d(0, 0, 1, 1deg);
    -moz-transform: rotate3d(0, 0, 1, 1deg);
    transform: rotate3d(0, 0, 1, 1deg);
  }
  50% {
    -webkit-transform: rotate3d(0, 0, 1, -1deg);
    -moz-transform: rotate3d(0, 0, 1, -1deg);
    transform: rotate3d(0, 0, 1, -1deg);
  }
  to {
    -webkit-transform: rotate3d(0, 0, 1, 1deg);
    -moz-transform: rotate3d(0, 0, 1, 1deg);
    transform: rotate3d(0, 0, 1, 1deg);
  }
}
