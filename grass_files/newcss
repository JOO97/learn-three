  top: 0;
  left: 0px;
  font-size: 14px;
  font-family: "Microsoft YaHei", tahoma, arial, "Hiragino Sans GB", "\5b8b\4f53", sans-serif;
  z-index: 9999;
}
.header .client-tip-icon {
  position: absolute;
  width: 18px;
  height: 19px;
  background-image: url(/images/header/client-tip-icon.png);
  z-index: 1002;
}
.header .icon-search-mi {
  display: inline-block;
  width: 30px;
  height: 30px;
  background-image: url(/images/header/search-icon.png);
  vertical-align: middle;
  background-size: cover;
  cursor: pointer;
}
